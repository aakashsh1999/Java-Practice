	public static void main(String args[])
			{
					String host;
					Scanner sc=new Scanner(System.in);
					System.out.println("Enter the Url");
					host=sc.nextLine();
					InetAddress address=new InetAddress(host);
					System.out.println("The Host name is" +getHostName());
					System.out.println("The Address is" +getHostAddress());
			}
}